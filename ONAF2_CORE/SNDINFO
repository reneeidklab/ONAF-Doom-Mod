$volume rochellejump 0.3

$random rochelleDie    { rochelleDie1 rochelleDie2 rochelleDie3 rochelleDie4 rochelleDie5 rochelleDie6 rochelleDie7}
rochelleDie1  rocDie1
rochelleDie2  rocDie2
rochelleDie3  rocDie3
rochelleDie4  rocDie4
rochelleDie5  rocDie5
rochelleDie6  rocDie6
rochelleDie7  rocDie7

$limit rochelleDie 0
$rolloff rochelleDie 512 1250
$volume rochelleDie 1.8

$random ClownStep {clowstp01 clowstp02 clowstp03 clowstp04}
clowstp01 CLOWSTP1
clowstp02 CLOWSTP2
clowstp03 CLOWSTP3
clowstp04 CLOWSTP4

$rolloff ClownStep 512 1024
$volume ClownStep 1.0

$random BeaverStep {beastep01 beastep02 beastep03 beastep04}
beastep01 BEASTEP1
beastep02 BEASTEP2
beastep03 BEASTEP3
beastep04 BEASTEP4

$rolloff BeaverStep 256 768
$volume BeaverStep 1.0

$random EyesaurStep {eyestep01 eyestep02 eyestep03 eyestep04 eyestep05 eyestep06}
eyestep01 EYESTEP1
eyestep02 EYESTEP2
eyestep03 EYESTEP3
eyestep04 EYESTEP4
eyestep05 EYESTEP5
eyestep06 EYESTEP6

$rolloff ClownStep 768 2048
$volume ClownStep 4.5